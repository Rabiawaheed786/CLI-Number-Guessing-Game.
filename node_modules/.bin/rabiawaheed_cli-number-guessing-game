#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -x "$basedir//" ]; then
  exec "$basedir//" usr/bin/env node "$basedir/../rabiawaheed_cli-number-guessing-game/index.js" "$@"
else 
  exec / usr/bin/env node "$basedir/../rabiawaheed_cli-number-guessing-game/index.js" "$@"
fi
